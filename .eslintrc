rules:
  no-console: 1
  no-underscore-dangle: 0
  indent:
    - 2
    - 2
  quotes:
    - 2
    - single
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - always
  eqeqeq: 
    - 2
    - smart
  no-use-before-define:
    - 2
    - nofunc
env:
  es6: true
  node: true
  mocha: true
extends: 'eslint:recommended'
